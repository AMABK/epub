{
    "name": "amabk/epub",
    "type": "library",
    "description": "A PHP library for reading and manipulating contents and metadata of EPUB files.",
    "keywords": [
        "EPUB",
        "PHP",
        "metadata"
    ],
    "homepage": "https://github.com/amabk/epub",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Gohr",
            "email": "andi@splitbrain.org"
        },
        {
            "name": "Simon Schrape",
            "email": "simon@epubli.com"
        },
        {
            "name": "Epubli Developers",
            "email": "devs@epubli.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-dom": "*",
        "ext-zip": "*",
        "epubli/common": "master"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Epubli\\Epub\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/epubli/common.git"
        }
    ]
}
